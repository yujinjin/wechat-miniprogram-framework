{
    "description": "微信小程序框架",
    "miniprogramRoot": "dist/",
    "compileType": "miniprogram",
    "libVersion": "2.12.1",
    "projectname": "wechat-miniprogram-framework",
    "setting": {
        "urlCheck": false,
        "es6": true,
        "enhance": true,
        "postcss": true,
        "preloadBackgroundData": false,
        "minified": true,
        "newFeature": true,
        "coverView": true,
        "nodeModules": true,
        "autoAudits": false,
        "showShadowRootInWxmlPanel": false,
        "scopeDataCheck": false,
